<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="installer.CompetitionPage"
             Title="Competition">
    <VerticalStackLayout>
        <Label 
            Text="This is the competition page."
            FontSize="24"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Label
            FontSize="24"
            Text="{OnPlatform unknown, WinUI=windows, iOS=ios, Android=android, MacCatalyst=mac}"
            VerticalOptions="End" />


        <Button
            x:Name="JumpBtn"
            Text="Home"
            Clicked="JumpBtn_Clicked" />
    </VerticalStackLayout>
</ContentPage>